package lesson4;

import java.util.Scanner;

public class ex9 {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int x=sc.nextInt();
        int s=0;
        while(x!=0){
            s++;
            x=sc.nextInt();
        }
        System.out.println(s);
    }
}
