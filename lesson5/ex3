package lesson5;

import java.util.Scanner;

public class ex3 {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int len=sc.nextInt();
        int[] array=new int[len];
        for(int i=0;i<len;i++)
            array[i]= sc.nextInt();
        int n= sc.nextInt();
        int s=0;
        for (int ar:array) {
            if(ar>n)
                s+=s;
        }
        System.out.println(s);
    }
}
