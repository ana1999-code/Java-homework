package lesson3;

import java.util.Scanner;

public class ex9 {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b= sc.nextInt();
        int c= sc.nextInt();
        System.out.println((a>0&&b<=0&&c<=0)||(b>0&&a<=0&&c<=0)||(c>0&&a<=0&&b<=0));
    }
}
