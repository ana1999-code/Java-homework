package lesson13;

public abstract class BaseEntity {
    protected long id;
    protected long version;

   /* public BaseEntity(long id, long version) {
        this.id = id;
        this.version=version;
    }*/

    public long getId() {
        return id;
    }

    public long getVersion() {
        return version;
    }

    public void setId(long id) {
        this.id = id;
    }

    public void setVersion(long version) {
        this.version = version;
    }
}
